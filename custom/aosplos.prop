# Add calibration file path
ro.config.calibration_cad=/system/etc/calibration_cad.xml

# Additional buffers shared between Camera and Video
vidc.enc.dcvs.extra-buff-count=2

# Forcing AUX for listed packages
vendor.camera.aux.packagelist=org.codeaurora.snapcam,com.android.camera,com.google.android.GoogleCamera,com.bsgmod.camera,org.lineage.snap,com.qualcomm.qti.qmmi
camera.aux.packagelist=org.codeaurora.snapcam,com.android.camera,com.google.android.GoogleCamera,com.bsgmod.camera,org.lineage.snap,com.qualcomm.qti.qmmi

# Forcing HAL1 for below packages
vendor.camera.hal1.packagelist=com.skype.raider,com.whatsapp,com.facebook.katana,com.gbwhatsapp,com.gbinsta
camera.hal1.packagelist=com.skype.raider,com.whatsapp,com.facebook.katana,com.gbwhatsapp,com.gbinsta

# HAL3 & EIS
persist.vendor.camera.HAL3.enabled=1
persist.camera.HAL3.enabled=1
persist.vendor.camera.eis.enable=0
persist.camera.eis.enable=0

# Properties for lux
persist.bokeh.switch.lux=290
persist.flash.low.lux=390
persist.flash.light.lux=340
persist.imx376_sunny.low.lux=310
persist.imx376_sunny.light.lux=280
persist.imx376_ofilm.low.lux=310
persist.imx376_ofilm.light.lux=280

# Dual Camera props
camera.lowpower.record.enable=1
persist.camera.auxswitch.threshold=330
persist.camera.depth.focus.cb=0
persist.camera.expose.aux=1
persist.camera.is_type=5
persist.camera.llnoise=1
persist.camera.linkpreview=0
persist.camera.isp.clock.optmz=0
persist.camera.isp.turbo=1
persist.camera.mainswitch.threshold=419
persist.camera.max.previewfps=60
persist.camera.preview.ubwc=0
persist.camera.set.afd=4
persist.camera.stats.test=0
persist.camera.tnr_cds=1
persist.camera.tnr.video=1
persist.camera.tnr.preview=1
persist.camera.tnr.snapshot=1
persist.tnr.process.plates=2
persist.denoise.process.plates=2